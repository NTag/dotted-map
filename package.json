{
  "name": "dotted-map",
  "version": "2.2.3",
  "type": "module",
  "description": "Create a SVG map filled with dots for the world or countries",
  "keywords": [
    "map",
    "svg",
    "dots",
    "dotted",
    "world",
    "countries",
    "country"
  ],
  "author": "Basile Bruneau <basile@bruneau.email>",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./without-countries": {
      "import": "./dist/without-countries.js",
      "require": "./dist/without-countries.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "lint": "biome check"
  },
  "dependencies": {
    "@turf/boolean-point-in-polygon": "^7.3.1",
    "proj4": "^2.6.1"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.8",
    "@types/bun": "latest",
    "@vitest/coverage-v8": "4.0.14",
    "tsdown": "^0.17.0-beta.4",
    "vitest": "^4.0.14"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "packageManager": "bun@1.3.1"
}
